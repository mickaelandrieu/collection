{
    "name": "pitchart/collection",
    "description": "A simple immutable collection library with functional capabilities",
    "type": "library",
    "license" : "MIT",
    "keywords": ["collection", "functional", "immutable"],
    "authors": [
        {
            "name": "Julien VITTE",
            "email": "vitte.julien@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Pitchart\\Collection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pitchart\\Collection\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0"
    }
}
